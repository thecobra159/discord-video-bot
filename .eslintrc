{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "standard",
      "prettier",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": [
      "no-console",
      "prettier/prettier"
    ]
  }